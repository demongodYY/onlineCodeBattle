{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "import-sources-order": "any",
    "named-imports-order": "any",
    "member-access": false,
    "no-console": false,
    "jsx-no-bind": false,
    "no-eval": false,
    "no-unused-variable": false
  }
}